{
	"name": "fastify-sequelize-ts",
	"version": "1.0.0",
	"description": "",
	"main": "app.ts",
	"directories": {
		"test": "test"
	},
	"prisma": {
		"schema": "./prisma/schema.prisma",
		"seed": "ts-node prisma/seeders/index.ts"
	},
	"scripts": {
		"local": "nodemon --watch src -e ts,json --exec \"rimraf dist && tsc --project tsconfig.json && tsc-alias && fastify start -a 0.0.0.0 -l info dist/app.js\"",
		"test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
		"start": "fastify start -a 0.0.0.0 -l info dist/app.js",
		"build:ts": "npm install && rm -rf dist && tsc --project tsconfig.json && tsc-alias",
		"watch:ts": "tsc -w",
		"dev": "nodemon --watch src -e ts,json --exec \"npm run build:ts && fastify start -a 0.0.0.0 -l info dist/app.js\"",
		"dev:start": "fastify start -a 0.0.0.0 --ignore-watch=.ts$ -w -l info -P dist/app.js",
		"prepare": "husky install",
		"model": "npx ts-node ./src/scripts/auto.ts",
		"migrate": "npx prisma migrate deploy",
		"seed": "npx prisma db seed",
		"migrate:create": "npx prisma migrate dev --create-only",
		"commit": "git add . && npx czg",
		"czg-retry": "git add . && npx czg --retry"
	},
	"keywords": [
		"fastify",
		"sequelize",
		"typescript"
	],
	"author": "ray",
	"license": "ISC",
	"dependencies": {
		"@fastify/autoload": "^5.0.0",
		"@fastify/cors": "^8.2.0",
		"@fastify/env": "^4.2.0",
		"@fastify/helmet": "^10.1.0",
		"@fastify/jwt": "^7.2.4",
		"@fastify/multipart": "^7.4.1",
		"@fastify/rate-limit": "^8.0.0",
		"@fastify/sensible": "^5.0.0",
		"@fastify/swagger": "^8.3.1",
		"@fastify/swagger-ui": "^1.4.0",
		"@prisma/client": "^4.12.0",
		"@scalar/fastify-api-reference": "1.25.10",
		"async": "^3.2.6",
		"axios": "^1.7.9",
		"bcrypt": "^6.0.0",
		"dotenv": "^16.0.3",
		"fastify": "^4.0.0",
		"fastify-cli": "^5.7.1",
		"fastify-plugin": "^4.0.0",
		"fastify-tsconfig": "^1.0.1",
		"fluent-json-schema": "^4.2.1",
		"fs": "^0.0.1-security",
		"husky": "^8.0.0",
		"mime-types": "^3.0.1",
		"moment": "^2.30.1",
		"node-cron": "^3.0.3",
		"path": "^0.12.7",
		"pg": "^8.16.3",
		"prisma": "^4.12.0",
		"rimraf": "^6.1.2",
		"sequelize": "^6.29.0",
		"stream-to-buffer": "^0.1.0",
		"ts-node": "^10.4.0",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.0.0",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/dotenv": "^8.2.0",
		"@types/mime-types": "^3.0.1",
		"@types/node": "^18.0.0",
		"@types/node-cron": "^3.0.11",
		"@types/pg": "^8.15.5",
		"@types/tap": "^15.0.5",
		"concurrently": "^7.0.0",
		"nodemon": "^3.0.2",
		"pretty-quick": "^3.1.3",
		"sequelize-auto": "^0.8.8",
		"tap": "^16.1.0"
	}
}
